Debug	include/myball.h	/^class Debug$/;"	c
Debug::err	include/myball.h	/^		char err[256];$/;"	m	class:Debug	access:public
Debug::pFile	include/myball.h	/^		FILE *pFile;$/;"	m	class:Debug	access:public
WINDOW_HEIGHT	include/myball.h	7;"	d
WINDOW_WIDTH	include/myball.h	6;"	d
_GAME_HPP_	include/myball.h	2;"	d
ball	include/myball.h	/^	ball()$/;"	f	class:ball	access:public	signature:()
ball	include/myball.h	/^class ball$/;"	c
ball::ball	include/myball.h	/^	ball()$/;"	f	class:ball	access:public	signature:()
ball::setFillColor	include/myball.h	/^		shape_.setFillColor(sf::Color::Red);$/;"	p	signature:(sf::Color::Red)
ball::shape_	include/myball.h	/^		sf::CircleShape shape_;$/;"	m	class:ball	access:public
ball::v2f_move_offset_	include/myball.h	/^		sf::Vector2f v2f_move_offset_;$/;"	m	class:ball	access:public
ball::v2f_position_	include/myball.h	/^		sf::Vector2f v2f_position_;$/;"	m	class:ball	access:public
buff	src/myball.cpp	/^	char buff[128];$/;"	l
clear	src/myball.cpp	/^			myGame.window.clear(sf::Color::Red);$/;"	p	file:	signature:(sf::Color::Red)
close	src/myball.cpp	/^					myGame.window.close();$/;"	p	file:	signature:()
display	src/myball.cpp	/^			myGame.window.display();$/;"	p	file:	signature:()
err	include/myball.h	/^		char err[256];$/;"	m	class:Debug	access:public
escKeyPressed	src/myball.cpp	/^	bool escKeyPressed;$/;"	l
main	src/myball.cpp	/^int main(int argc, char const* argv[])$/;"	f	signature:(int argc, char const* argv[])
myGame	src/myball.cpp	/^	Game myGame;$/;"	l
pFile	include/myball.h	/^		FILE *pFile;$/;"	m	class:Debug	access:public
setFillColor	include/myball.h	/^		shape_.setFillColor(sf::Color::Red);$/;"	p	signature:(sf::Color::Red)
shape_	include/myball.h	/^		sf::CircleShape shape_;$/;"	m	class:ball	access:public
